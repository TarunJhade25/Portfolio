{
    "version":2,
    "builds":[
        {
            "src":"Portfolio/wsgi.py",
            "use":"@vercel/python",
            "config":{"maxLambdaSize": "15mb", "runtime":"python"}
        }
    ],
    "routes":[
        {
            "src":"/static/(.*)",
            "dest":"/static/$1"
        },
        {
            "src":"/(.*)",
            "dest":"Portfolio/wsgi.py"
        }
    ]
}